<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.WorkCycle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icons"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_rounds"
        android:supportsRtl="true"
        android:theme="@style/Theme.LofginPage">
        <activity
            android:name="com.example.WorkCycle.clientdetails"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.forgot"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.pedit"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.clientdata"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.UpdateData"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.deletepage"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.eventviewpage"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.cilentdash"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.Adminlog"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.Eventcreatepage"
            android:exported="false" />
        <activity
            android:name="com.example.WorkCycle.Dashboard"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.LofginPage.NoActionBar" />
        <activity
            android:name="com.example.WorkCycle.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.WorkCycle.sign_in"
            android:exported="false"
            android:parentActivityName="com.example.WorkCycle.MainActivity" />
        <activity
            android:name="com.example.WorkCycle.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>